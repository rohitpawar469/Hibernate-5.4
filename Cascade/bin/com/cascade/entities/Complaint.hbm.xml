<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cascade.entities">
	<class name="Complaint" table="complaint">
		<id name="complaintNo" column="complaint_no">
			<generator class="increment"></generator>
		</id>
		<property name="title" />
		<property name="description" />
		<property name="reportedDate" column="reported_date" />
		<property name="severity" />
		<property name="priority" />
		<property name="status" column="complaint_status" />
	</class>
</hibernate-mapping>