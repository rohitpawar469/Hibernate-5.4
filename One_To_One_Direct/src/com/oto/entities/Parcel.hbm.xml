<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.oto.entities">
	<class name="Parcel" table="parcel">
		<id name="awbNo" column="awb_No">
			<generator class="increment"></generator>
		</id>
	<property name="shippedDate" column="shipped_date" />
	<property name="description"></property>
	<property name="source" />
	<property name="destination" />
	<property name="senderName" column="sender_name" />
	<property name="receiverName" column="receiver_name"></property>
	<property name="amount" />
	</class>
</hibernate-mapping>	